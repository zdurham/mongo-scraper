<div class="navbar-fixed">
  <nav>
    <div class="nav-wrapper">
      <a href="#!" class="brand-logo">Logo</a>
      <ul class="right hide-on-med-and-down">
        <li><a href="sass.html">Link 1</a></li>
        <li><a href="badges.html">Link 2</a></li>
        <li>
          <form action='/scrape' method='get'>
            <button class="btn waves-effect waves-light" type="submit" id='scrape' name="action">Scrape New Articles!
            </button>
          </form>
        </li>
      </ul>
    </div>
  </nav>
</div>

<div class='container'>
  <div class='row'>
    <div clas='col s12'>
      <p>Hello World! This is working!</p>
    </div>
  </div>
  <div class='row'>
    <div class='col s12'>
      <ul class='articles'>Articles</ul>
        {{#each articles}}
          <li class='article'>
            <p>Title: {{this.title}}</p> 
            <img src={{this.url}} style='height:100px;width:100px;'>
            <p>Summary: {{this.summary}}</p>
            <p>Link: {{this.link}}</p>
          </li>
        {{/each}}
    </div>
  </div>
</div>
        









