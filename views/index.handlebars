

<div class='row'>
  <div clas='col s12'>
    <p>Hello World! This is working!</p>
  </div>
</div>
<div class='row'>
  <div class='col s12'>
    <div class='articles'>Latest Atlantic Articles</ul>
      {{#each articles}}
        <div class='card horizontal'>
          <div class='card-image'>
            <img src={{this.img}}>
          </div>
          <div class='card-stacked'>
            <div class='card-content'>
              <div class='card-title'>{{this.title}}</div>
              <p>{{this.summary}}</p>
            </div>
            <div class='card-action'>
              <a href={{this.link}}>Link</a>
            </div>
          </div>
        </div>
      {{/each}}
  </div>
</div>

        









